  <rdf:Description rdf:about="https://cim4.eu/ns/nc#PowerElectronicsUnit.p">
    <rdfs:comment>Active power injection. Load sign convention is used, i.e. positive sign means flow out from a node.
Starting value for a steady state solution.</rdfs:comment>
    <cims:dataType rdf:resource="#ActivePower"/>
    <rdfs:domain rdf:resource="#PowerElectronicsUnit"/>
    <rdfs:label xml:lang="en">p</rdfs:label>
