    <rdfs:subClassOf rdf:resource="#ConductingEquipment"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://cim4.eu/ns/nc#ACDCConverterAction">
    <cims:belongsToCategory rdf:resource="https://ap-voc.cim4.eu/SteadyStateInstruction#Package_DocSteadyStateInstructionProfile"/>
    <rdfs:comment>Alternate current Direct current (ACDC) converter action.</rdfs:comment>
    <rdfs:label xml:lang="en">ACDCConverterAction</rdfs:label>
