  <rdf:Description rdf:about="https://cim4.eu/ns/nc#FrequencyControlFunction.targetValue">
    <rdfs:comment>Target value for the frequency that the control function is calculating to achieve by adjusting the operational setting to the controlled equipment.</rdfs:comment>
    <cims:dataType rdf:resource="#Frequency"/>
    <rdfs:domain rdf:resource="https://cim4.eu/ns/nc#FrequencyControlFunction"/>
    <rdfs:label xml:lang="en">targetValue</rdfs:label>
    <cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:1..1"/>
    <cims:stereotype rdf:resource="http://iec.ch/TC57/NonStandard/UML#attribute"/>
    <cims:stereotype>NC</cims:stereotype>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://cim4.eu/ns/nc#FuelStorage">
    <cims:belongsToCategory rdf:resource="https://ap-voc.cim4.eu/SteadyStateInstruction#Package_SteadyStateInstructionProfile"/>
    <rdfs:comment>Fuel storage. e.g. pile of coal that can be shared between multiple thermal generating units.</rdfs:comment>
    <rdfs:label xml:lang="en">FuelStorage</rdfs:label>
    <cims:stereotype>NC</cims:stereotype>
    <cims:stereotype>Description</cims:stereotype>
    <cims:stereotype rdf:resource="http://iec.ch/TC57/NonStandard/UML#concrete"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://cim4.eu/ns/nc#FuelStorage.energyStorage">
    <rdfs:comment>Amount of energy available in the storage.</rdfs:comment>
    <cims:dataType rdf:resource="#RealEnergy"/>
    <rdfs:domain rdf:resource="https://cim4.eu/ns/nc#FuelStorage"/>
    <rdfs:label xml:lang="en">energyStorage</rdfs:label>
    <cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:1..1"/>
    <cims:stereotype rdf:resource="http://iec.ch/TC57/NonStandard/UML#attribute"/>
    <cims:stereotype>NC</cims:stereotype>
