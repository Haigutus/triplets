  <rdf:Description rdf:about="https://cim4.eu/ns/nc#FACTSEquipment">
    <cims:belongsToCategory rdf:resource="https://ap-voc.cim4.eu/SteadyStateInstruction#Package_SteadyStateInstructionProfile"/>
    <rdfs:comment>Flexible Alternating Current Transmission System regulating equipment.</rdfs:comment>
    <rdfs:label xml:lang="en">FACTSEquipment</rdfs:label>
    <cims:stereotype>NC</cims:stereotype>
    <rdfs:subClassOf rdf:resource="#RegulatingCondEq"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://cim4.eu/ns/nc#FACTSEquipment.q">
    <rdfs:comment>Reactive power injection. Load sign convention is used, i.e. positive sign means flow out from a node.
Starting value for a steady state solution.</rdfs:comment>
    <cims:dataType rdf:resource="#ReactivePower"/>
