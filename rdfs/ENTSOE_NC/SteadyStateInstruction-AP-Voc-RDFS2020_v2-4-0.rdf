    <cims:belongsToCategory rdf:resource="https://ap-voc.cim4.eu/SteadyStateInstruction#Package_SteadyStateInstructionProfile"/>
    <rdfs:comment>Function block is a function described as a set of elementary blocks. The blocks describe the function between input variables and output variables.</rdfs:comment>
    <rdfs:label xml:lang="en">FunctionBlock</rdfs:label>
    <cims:stereotype>NC</cims:stereotype>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://cim4.eu/ns/nc#FunctionBlock.enabled">
    <rdfs:comment>True, if the function block is enabled (active). Otherwise false.</rdfs:comment>
    <cims:dataType rdf:resource="#Boolean"/>
    <rdfs:domain rdf:resource="https://cim4.eu/ns/nc#FunctionBlock"/>
    <rdfs:label xml:lang="en">enabled</rdfs:label>
    <cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:1..1"/>
    <cims:stereotype rdf:resource="http://iec.ch/TC57/NonStandard/UML#attribute"/>
    <cims:stereotype>NC</cims:stereotype>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  </rdf:Description>
