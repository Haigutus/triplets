    <cims:stereotype rdf:resource="http://iec.ch/TC57/NonStandard/UML#attribute"/>
    <cims:stereotype>NC</cims:stereotype>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://cim4.eu/ns/nc#BiddingZoneBorder.alreadyAllocatedFlow">
    <rdfs:comment>The maximum allowed flow on the collection of interconnection between two bidding zones.</rdfs:comment>
    <cims:dataType rdf:resource="#ActivePower"/>
    <rdfs:domain rdf:resource="https://cim4.eu/ns/nc#BiddingZoneBorder"/>
    <rdfs:label xml:lang="en">alreadyAllocatedFlow</rdfs:label>
    <cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:0..1"/>
